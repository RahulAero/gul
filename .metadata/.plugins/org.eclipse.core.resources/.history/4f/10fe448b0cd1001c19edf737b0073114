package org.robo;

public class Prime {
	 
	static String  CheckingPrimeno(int a) {
		int b= 0;
		for (int i = 0; i < a; i++) {
			if (a%i == 0) {
				b++;
				break;
				
			}
			if (b==0) {
				return "Prime no";
				
				
			}else {
				return "Not a prime number";
				
						
			}
		}
	}
			public static void main(String[] args) {
			String CheckingPrimenom = CheckingPrimeno(123);
			System.out.println("checking PrimeNo");
			
			
		}
		
		
		
		

	}

}
